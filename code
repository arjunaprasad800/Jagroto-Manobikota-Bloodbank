global bloodbank(blooduser):
@local blooduser(username,serialno,imageno,age,group,sex1,sex2,bloodgroup,donationtime):
def registry():
    name = username()
    for name in range(username):
        print(username)
    serial= serialno()
    if serial in range(serialno):
        print(serial)
    else:
        print("Not Found")
    captionno= imageno()
    while captionno in imageno:
        print(captionno)
    else:
        print("Not Found")
    age = int()
    for age in range(1,100):
        print(age)
    else:
        print("Error; Check Again")
    sex1 = "Male"
    sex2= "Female"
    if sex1 == "Male":
        print("Male")
    elif sex2== "Female":
        print("Female")
    else:
        print("Error Occured")
    bloodgroup= Apositive,Anegative,Bpositive,Bnegative,ABpositive,ABnegative,Opositive,Onegative
    for i in range(bloodgroup):
        print(bloodgroup)
        if bloodgroup==Apositive:
            print(bloodgroup)
        elif bloodgroup==Anegative:
            print(Anegative)
        elif bloodgroup==Bpositive:
            print(Bpositive)
        elif bloodgroup==Bnegative:
            print(Bnegative)
        elif bloodgroup==ABpositive:
            print(ABpositive)
        elif bloodgroup==ABnegative:
            print(ABnegative)
        elif bloodgroup==Opositive:
            print(Opositive)
        elif bloodgroup==Onegative:
            print(Onegative)
        else:
            print("Error,Check Again")
        
    x= donationtime.append()
    donationtime= int()
    return donationtime
    return registry
@local




